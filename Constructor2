class Student {
	int id;
	String name;

	Student(int i,String n) {
	id=i;
	name=n;
	}

	void display() {
	System.out.println(id+" "+name);
	}
}

	public class paramConstrDemo {
		public static void main(String[] args) {

			Student s1=new Std(100,"Ram");
			Student s2=new Std(99,"Sam");
			s1.display();
			s2.display();
		}
}
